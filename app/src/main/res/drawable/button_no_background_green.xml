<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid
                android:color="#2a747474" />
        </shape>
    </item>
    <item>
        <shape>
            <solid
                android:color="#fff"/>
            <stroke
                android:color="#419C28"
                android:width="1dp" />
            <corners
                android:radius="5dp"/>
        </shape>
    </item>
</selector>
